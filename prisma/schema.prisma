generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Venta {
  id          Int      @id @default(autoincrement())
  producto    String   @db.VarChar(255)
  categoria   String
  monto       Float
  fecha       DateTime @default(now())
  region      String?

  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}